{
  "name": "messenger",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "scripts": {
    "build": "npm run build:all",
    "build:assets": "ts-node src/tools/buildAssets",
    "build:client": "webpack --progress",
    "build:server": "tsc",
    "build:views": "ts-node src/tools/buildViews",
    "build:all": "npm-run-all clean lint build:server build:assets build:views build:client",
    "build:all:start": "npm-run-all build:all start",
    "clean": "rimraf dist/*",
    "dev": "nodemon --watch src -e ts,ejs,tsx,js --exec npm run build:all:start",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "postinstall": "cp node_modules/webrtc-adapter/out/adapter.js ./public/javascript/adapter.js",
    "start": "node dist/server.js",
    "start:all": "npm run dev",
    "start:client": "nodemon --watch src -e ts,ejs,tsx,js --exec npm-run-all build:client start",
    "start:client:server": "nodemon --watch src -e ts,ejs,tsx,js --exec npm-run-all build:client build:server start",
    "start:server": "nodemon --watch src -e ts,ejs,tsx,js --exec npm-run-all build:server start",
    "test": "jest --detectOpenHandles"
  },
  "dependencies": {
    "aws-sdk": "^2.1187.0",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "db-migrate-pg": "^1.2.2",
    "debug": "^2.6.9",
    "dotenv": "^16.0.1",
    "ejs": "~2.6.1",
    "express": "^4.16.4",
    "express-fileupload": "^1.4.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "rimraf": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sequelize": "^6.21.3",
    "sequelize-cli": "^6.4.1",
    "shelljs": "^0.8.5",
    "ts-dotenv": "^0.8.3",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webrtc-adapter": "^1.4.0",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@aws-sdk/types": "^3.127.0",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.2.3",
    "@types/fs-extra": "^9.0.13",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^28.1.6",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.0.3",
    "@types/shelljs": "^0.8.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "fs-extra": "^10.1.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "nodemon": "^2.0.19",
    "superagent": "^8.0.0",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.6",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4",
    "typesync": "^0.9.2"
  }
}
